{
  "name": "vue-youtube-playlist",
  "version": "1.0.2",
  "description": "This is a simple component, which allows you to display the youtube iframe embed - with a playlist style sidebar to change between multiple videos.",
  "author": "Owen Melbourne <owenmelbz@gmail.com>",
  "main": "src/components/YouTubePlaylist.vue",
  "scripts": {
    "build": "run-s build:**",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "build:autoprefixer": "postcss --use autoprefixer -o dist/assets/css/app.css dist/assets/css/app.css",
    "build:cssnano": "cssnano dist/assets/css/app.css dist/assets/css/app.css",
    "serve": "cross-env NODE_ENV=development rollup -cw"
  },
  "dependencies": {
    "perfect-scrollbar": "^1.4.0",
    "query-string": "^6.1.0",
    "url-parse": "^1.4.3",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "cross-env": "^5.0.5",
    "cssnano-cli": "^1.0.4",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^5.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.1",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-vue": "^3.0.0",
    "vue-template-compiler": "^2.5.13"
  }
}
